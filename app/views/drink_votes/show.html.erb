<div class="page-header">
  <h3>Drink Vote #<%= @drink_vote.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @drink_vote.user_id %></dd>

      <dt>Drink</dt>
      <dd><%= @drink_vote.drink_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/drink_votes" class="btn btn-primary">
        Back
      </a>
      <a href="/drink_votes/<%= @drink_vote.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_drink_vote/<%= @drink_vote.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
