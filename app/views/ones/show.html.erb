<div class="page-header">
  <h3>One #<%= @one.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @one.user_id %></dd>

      <dt>Name</dt>
      <dd><%= @one.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/ones" class="btn btn-primary">
        Back
      </a>
      <a href="/ones/<%= @one.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_one/<%= @one.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
